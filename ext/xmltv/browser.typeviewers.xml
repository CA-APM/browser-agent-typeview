<?xml version="1.0" encoding="UTF-8"?>
<TypeViewerSet namespace="Browser" version="2.0" xmlns="wily.typeviews" xmlns:its="http://www.w3.org/2005/11/its">
    <!-- Component Definitions - Start -->
    <ComponentDefinition id="Browser-graph">
        <Panel columns="1" layout="GridLayout" rows="2">
            <Graph relative-specifier=":Average .*">
                <Title its:translate="yes">Browser Timing (ms)</Title>
            </Graph>
			<Panel columns="2" layout="GridLayout" rows="1">
				<Graph relative-specifier=":Page Hits Per Interval">
					<Title its:translate="yes">Page Hits Per Interval</Title>
				</Graph>
				<Graph relative-specifier=":JavaScript Errors Per Interval">
					<Title its:translate="yes">JavaScript Errors Per Interval</Title>
				</Graph>
			</Panel>
        </Panel>
    </ComponentDefinition>
    <ComponentDefinition id="ajax-graph">
        <Panel columns="1" layout="GridLayout" rows="2">
            <Graph relative-specifier=":Average .*">
                <Title its:translate="yes">AJAX Timing (ms)</Title>
            </Graph>
			<Graph relative-specifier=":Invocation Count Per Interval">
				<Title its:translate="yes">Invocation Count Per Interval</Title>
			</Graph>
        </Panel>
    </ComponentDefinition>
    <ComponentDefinition id="spa-graph">
        <Graph relative-specifier=":Average .*">
            <Title its:translate="yes">Browser Timing (ms)</Title>
        </Graph>
    </ComponentDefinition>
    <ComponentDefinition id="BS-bt">
        <Panel layout="BorderLayout" contraint="Center">
            <Title>Business Transactions</Title>
		<MetricDataTable show-bottom-graph="false">
			<Title>Business Transactions</Title>
			<Column content-type="LastPrefixSegment" relative-width="3" segment-offset="3">
				<Title its:translate="yes">Service</Title>
			</Column>
			<Column content-type="LastPrefixSegment" relative-width="3" segment-offset="2">
				<Title its:translate="yes">Transaction</Title>
			</Column>
			<Column content-type="NumericalValue" relative-specifier=":Average Page Load Time \(ms\)" sort="Descending">
				<Title its:translate="yes">Avg Page Load Time</Title>
			</Column>
            <Column content-type="NumericalValue" relative-specifier=":Average Page Render Time \(ms\)">
				<Title its:translate="yes">Avg Page Render Time</Title>
			</Column>
            <Column content-type="NumericalValue" relative-specifier=":Average DOM Processing Time \(ms\)">
				<Title its:translate="yes">Avg DOM Processing Time</Title>
			</Column>
            <Column content-type="NumericalValue" relative-specifier=":Average Connection Establishment Time \(ms\)">
				<Title its:translate="yes">Avg Connection Time</Title>
			</Column>
            <Column content-type="NumericalValue" relative-specifier=":Average Domain Lookup Time \(ms\)">
				<Title its:translate="yes">Avg Domain Lookup Time</Title>
			</Column>
            <Column content-type="NumericalValue" relative-specifier=":JavaScript Errors Per Interval">
				<Title its:translate="yes">JavaScript Errors</Title>
			</Column>
			<Column content-type="NumericalValue" relative-specifier=":Page Hits Per Interval">
				<Title its:translate="yes">Page Hits</Title>
			</Column>
		</MetricDataTable>
        </Panel>
    </ComponentDefinition>
    <ComponentDefinition id="BS-url">
        <Panel layout="BorderLayout" contraint="Center">
            <Title>Browser Pages</Title>
		<MetricDataTable show-bottom-graph="false">
			<Title>Browser Pages</Title>
			<Column content-type="LastPrefixSegment" relative-width="3" segment-offset="1">
				<Title its:translate="yes">Host</Title>
			</Column>
			<Column content-type="LastPrefixSegment" relative-width="3" segment-offset="0">
				<Title its:translate="yes">URL</Title>
